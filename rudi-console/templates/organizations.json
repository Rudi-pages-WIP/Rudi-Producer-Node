{
    "htmlJsonTemplate": [
        { "tag": "form-header", "children": [
            { "id": "title",          "tag": "h1" },
            { "id": "@header_actions", "tag": "div" }
        ]},
        { "tag": "hr" },
        { "tag": "form-body", "children": [
            { "tag":"form-section", "class":["row"], "children": [
                { "id": "global",                "tag": "h1"           },
                { "id": "@organization_id",      "tag": "text-input",  "class": ["col","s4","m4","l6"],  "attr": { "required": true,  "disabled": true } },
                { "id": "@organization_name",    "tag": "text-input",  "class": ["col","s4","m4","l6"],  "attr": { "required": true } },
                { "id": "@organization_address", "tag": "text-input",  "class": ["col","s4","m4","l6"]   }
            ]}
        ]},
        { "tag": "form-footer", "children": [
            { "id": "@submit_btn", "tag": "button" }
        ]}
    ],
    "sumbitTemplate": {
        "organization_id": "organization_id",
        "organization_name": "organization_name",
        "organization_address": "organization_address"
    },
    "fragmentSet": {
        "fr": {
            "title":                    { "textContent": "Producteur De Données" },
            "global":                   { "textContent": "Général"       },
            "submit_btn":               { "textContent": "Valider"       },
            "organization_id":          { "attr": { "label": "Identifiant de l'organisation",  "helper": "Identifiant de l'organisation qui a produit le jeu de données dans RUDI" } },
            "organization_name":        { "attr": { "label": "Nom de l'organisation",          "helper": "Nom de l'organisation qui a produit le jeu de données"                   } },
            "organization_address":     { "attr": { "label": "Adresse",                        "helper": "Adresse de l'organisation qui a produit le jeu de données"               } }
        }
    }
}